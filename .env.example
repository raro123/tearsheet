# Cloudflare R2 Configuration (used by mktdata)
R2_ACCOUNT_ID=your_account_id
R2_ACCESS_KEY_ID=your_access_key
R2_SECRET_ACCESS_KEY=your_secret_key
R2_ENDPOINT_URL=your_account_id.r2.cloudflarestorage.com
R2_BUCKET_NAME=your_bucket_name
R2_REGION=auto

# Cache Configuration
# TTL (Time To Live) for data cache in hours. After this period, data is automatically refreshed.
# Default: 24 hours
CACHE_TTL_HOURS=24

# Note: R2 parquet file paths are configured in ~/.mktdata/config.yaml
# See mktdata documentation for catalog configuration
